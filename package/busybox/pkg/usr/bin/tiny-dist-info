#!/bin/sh
[ -f /sys/firmware/devicetree/base/model ] && machine=`cat /sys/firmware/devicetree/base/model`
ostype=`sysctl -n kernel.ostype`
osrelease=`sysctl -n kernel.osrelease`
busybox=`busybox | head -n 1 | cut -d " " -f 1-2`
version=`cat /etc/version`
echo ------------------------------------
[ "x${machine}" != x ] && echo "[$machine]"
echo $ostype $osrelease
echo $busybox
echo tiny-root $version
echo ------------------------------------
